<div class="menus">	
  <div class="wrap">			       	
    <div class="section_menus group_menus menus">
      @foreach (var item in ViewBag.Menus){
      <div class="grid_1_of_3 images_1_of_3 top_grid">
        <h3 @ViewHelper.Edit(item,"Title")>@item.Title </h3>
        <img src="@item.Img" alt="" />
        <p @ViewHelper.Edit(item,"Summary")>@item.Summary</p>
        <div class="button"><span><a href="@Url.FrontUrl().PageUrl("Menus/Detail",new{UserKey = item.UserKey})">Read More</a></span></div>
      </div>
      }                      
    </div>
