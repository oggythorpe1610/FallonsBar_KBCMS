<div class="bookings">	
  <div class="wrap">			       	
    <div class="section_book group_book bookings">
      @foreach (var item in ViewBag.Bookings)
      {
        <div class="grid_1_of_3 images_1_of_3 top_grid" style="min-height: 800px;">
            <h3 @ViewHelper.Edit(item, "Title")>@item.Title </h3>
            <p @ViewHelper.Edit(item, "Summary")>@item.Summary</p>
        </div>
        <div class="grid_1_of_3 images_1_of_3 top_grid">
            <iframe height="890" width="560" src="http://myseat.fallons-bar.co.uk/api/reserve.php?propertyID=1" scrolling="yes" class="frame">
                <p>Your browser does not support iframes.</p>
            </iframe>
        </div>
      }                      
    </div>
  </div>
</div>