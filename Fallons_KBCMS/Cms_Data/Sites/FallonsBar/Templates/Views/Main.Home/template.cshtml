 <div class="wrap">
		   <div class="content">	 	 
		       <div class="top-grids">
			 		<div class="section group">
          @foreach (var item in ViewBag.Services){
						<div class="grid_1_of_3 images_1_of_3 top_grid">
							  <h3 @ViewHelper.Edit(item,"Title")>@item.Title </h3>
							   <img src="@item.Img" alt="" />
							  <p @ViewHelper.Edit(item,"Summary")>@item.Summary</p>
						     <div class="button"><span><a href="@Url.FrontUrl().PageUrl("Services/Detail",new{UserKey = item.UserKey})">Read More</a></span></div>
						</div>
        }                      
					</div>
		 		</div>
			 	    <div class="mid-grid">
                      @Html.FrontHtml().RenderHtmlBlock("Mid_Grid")
			  		</div>			  	   
			 	</div>
		   	</div>		   			